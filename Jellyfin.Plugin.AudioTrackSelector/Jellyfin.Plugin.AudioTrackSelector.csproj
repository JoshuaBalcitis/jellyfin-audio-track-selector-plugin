<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <RootNamespace>Jellyfin.Plugin.AudioTrackSelector</RootNamespace>
    <AssemblyName>Jellyfin.Plugin.AudioTrackSelector</AssemblyName>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Nullable>enable</Nullable>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <Version>2.1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <!-- Jellyfin 10.11.6 Dependencies -->
    <PackageReference Include="Jellyfin.Controller" Version="10.11.6">
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Jellyfin.Model" Version="10.11.6">
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
  </ItemGroup>

</Project>
